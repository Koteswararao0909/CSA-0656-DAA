#include <stdio.h>
void printDetails() {
    printf("Name : P. Koteswara Rao\n");
    printf("Reg no : 192211581\n");
}
void printFactors(int n) {
    printf("Factors of %d are: ", n);
    for (int i = 1; i <= n; i++) {
        if (n % i == 0) {
            printf("%d ", i);
        }
    }
    printf("\n");
}
int main() {
    int n;
    printDetails();
    printf("Enter a number: ");
    scanf("%d", &n);
    printFactors(n);
    return 0;
}
